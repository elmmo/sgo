{
    "name": "sgo",
    "version": "1.0",
    "description": "Fixes auth issues and presents office hours",
    "permissions": ["tabs", "cookies", "*://*.whitworth.edu/*"],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "content_scripts": [
      {
        "matches": ["*://login.whitworth.edu/*", "*://blackboard.whitworth.edu/*"],
        "js": ["connections.js"]
      }
    ],
    "manifest_version": 2
  }